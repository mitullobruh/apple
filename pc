local function updateIDCollectList()
	local playerList = {};
	local seen = {};
	local Map = workspace:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\174\86\192", "\95\227\55\176\117\61"));
	if Map then
		for _, obj in ipairs(Map:GetChildren()) do
			if (obj.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\49\122", "\203\120\30\67\43")) then
				local prompt = obj:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\216\33\125\253\214\252\53\89", "\185\145\69\45\143"));
				if (prompt and prompt.Enabled) then
					local playerName = prompt:GetAttribute(LUAOBFUSACTOR_DECRYPT_STR_0("\186\19\24\191\217\152\49\24\171\217", "\188\234\127\121\198"));
					if (playerName and (playerName ~= LocalPlayer.Name) and not seen[playerName]) then
						local player = Players:FindFirstChild(playerName);
						if player then
							seen[playerName] = true;
							table.insert(playerList, playerName .. LUAOBFUSACTOR_DECRYPT_STR_0("\120\122", "\227\88\82\115") .. player.DisplayName .. ")");
						end
					end
				end
			end
		end
	end
	table.sort(playerList);
	SelectiveIDCollect:SetValues(playerList);
end
SelectiveIDCollect:OnChanged(function()
	local selectedName = SelectiveIDCollect.Value;
	if not selectedName then
		return;
	end
	local playerName = selectedName:match(LUAOBFUSACTOR_DECRYPT_STR_0("\125\87\129\153\74\78\8\86", "\19\35\127\218\199\98")):gsub(LUAOBFUSACTOR_DECRYPT_STR_0("\89\232\65\166", "\130\124\155\106"), "");
	local humanoidRootPart = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\253\222\251\174\173\249\117\187\231\196\249\187\147\247\110\171", "\223\181\171\150\207\195\150\28"));
	if not humanoidRootPart then
		Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\120\51\247\162\12", "\105\44\90\131\206")]="âŒ Error",[LUAOBFUSACTOR_DECRYPT_STR_0("\220\239\188\173\13\48\235", "\94\159\128\210\217\104")]=LUAOBFUSACTOR_DECRYPT_STR_0("\115\241\7\173\94\124\237\127\66\185\8\176\75\63\255\117\69\247\2", "\26\48\153\102\223\63\31\153"),[LUAOBFUSACTOR_DECRYPT_STR_0("\38\85\255\242\22\73\226\253", "\147\98\32\141")]=3});
		SelectiveIDCollect:SetValue(nil);
		return;
	end
	local Map = workspace:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\53\66\243", "\43\120\35\131\170\102\54"));
	if not Map then
		SelectiveIDCollect:SetValue(nil);
		return;
	end
	local originalPosition = humanoidRootPart.CFrame;
	local originalCameraType = Camera.CameraType;
	local originalCameraCFrame = Camera.CFrame;
	Camera.CameraType = Enum.CameraType.Scriptable;
	local found = false;
	for _, obj in ipairs(Map:GetChildren()) do
		if (obj.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\125\2", "\228\52\102\231\214\197\208")) then
			local surfaceGui = obj:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\45\245\103\204\235\136\28\241\11\233", "\182\126\128\21\170\138\235\121"));
			local prompt = obj:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\162\222\5\244\137\30\32\18", "\102\235\186\85\134\230\115\80"));
			if (surfaceGui and prompt and surfaceGui.Enabled and prompt.Enabled) then
				local promptPlayerName = prompt:GetAttribute(LUAOBFUSACTOR_DECRYPT_STR_0("\103\0\63\70\119\198\12\86\1\59", "\66\55\108\94\63\18\180"));
				if (promptPlayerName == playerName) then
					found = true;
					local idPosition = obj.Position;
					local idCFrame = obj.CFrame;
					local lookVector = idCFrame.LookVector;
					local characterPosition = idPosition + (lookVector * 3);
					humanoidRootPart.CFrame = CFrame.new(characterPosition, idPosition);
					local cameraPosition = (characterPosition - (lookVector * 2)) + Vector3.new(0, 2, 0);
					Camera.CFrame = CFrame.new(cameraPosition, idPosition);
					task.wait(0.1);
					for i = 1, 3 do
						prompt.HoldDuration = 0;
						prompt.MaxActivationDistance = 20;
						prompt.RequiresLineOfSight = false;
						fireproximityprompt(prompt);
						task.wait(0.1);
					end
					break;
				end
			end
		end
	end
	humanoidRootPart.CFrame = originalPosition;
	Camera.CameraType = originalCameraType;
	Camera.CFrame = originalCameraCFrame;
	if found then
		Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\32\132\145\59\34", "\57\116\237\229\87\71")]="âœ… ID Collected",[LUAOBFUSACTOR_DECRYPT_STR_0("\137\190\227\243\114\224\83", "\39\202\209\141\135\23\142")]=(LUAOBFUSACTOR_DECRYPT_STR_0("\204\38\10\9\55\235\236\53\28\6\62\225\191\48\6\6\62\253\252\39\12\14\114", "\152\159\83\105\106\82") .. playerName .. "'s ID"),[LUAOBFUSACTOR_DECRYPT_STR_0("\165\211\67\243\221\85\142\200", "\60\225\166\49\146\169")]=3});
	else
		Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\27\23\59\38\4", "\103\79\126\79\74\97")]="âŒ ID Not Found",[LUAOBFUSACTOR_DECRYPT_STR_0("\153\112\221\103\91\20\174", "\122\218\31\179\19\62")]=(LUAOBFUSACTOR_DECRYPT_STR_0("\144\217\216\205\205\225\75\188\194\141\199\192\175\65\243", "\37\211\182\173\161\169\193") .. playerName .. "'s ID"),[LUAOBFUSACTOR_DECRYPT_STR_0("\211\47\95\216\60\114\182\249", "\217\151\90\45\185\72\27")]=3});
	end
	SelectiveIDCollect:SetValue(nil);
end);

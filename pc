local InstantKill = Tabs.KiraFeatures:AddToggle(LUAOBFUSACTOR_DECRYPT_STR_0("\210\160\161\60\250\160\166\3\242\162\190", "\72\155\206\210"), {[LUAOBFUSACTOR_DECRYPT_STR_0("\114\115\64\2\54", "\83\38\26\52\110")]=LUAOBFUSACTOR_DECRYPT_STR_0("\113\25\52\82\89\25\51\6\115\30\43\74", "\38\56\119\71"),[LUAOBFUSACTOR_DECRYPT_STR_0("\215\234\75\213\55\95\227\251\81\217\43", "\54\147\143\56\182\69")]=LUAOBFUSACTOR_DECRYPT_STR_0("\251\128\244\76\204\150\165\250\72\203\222\193\209\70\203\211\193\244\64\211\218\146\191\64\209\197\149\254\71\203\150\201\241\70\159\222\142\243\77\150", "\191\182\225\159\41"),[LUAOBFUSACTOR_DECRYPT_STR_0("\15\23\46\84\158\139\214", "\162\75\114\72\53\235\231")]=false});
local function setupInstantKill(gui, enabled)
	local killButton;
	for _, btn in ipairs(gui:GetDescendants()) do
		if (btn:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\184\57\92\246\113\23\152\40\75\236", "\98\236\92\36\130\51")) and (btn.Text == LUAOBFUSACTOR_DECRYPT_STR_0("\140\54\32\158\5\128\144\2\129\89\56\149\5\141\153\25\137\48\34\155\113\141", "\80\196\121\108\218\37\200\213"))) then
			killButton = btn;
			break;
		end
	end
	if not killButton then
		return;
	end
	local holdScript = killButton:FindFirstChildOfClass(LUAOBFUSACTOR_DECRYPT_STR_0("\44\124\1\126\71\61\137\18\122\18\107", "\234\96\19\98\31\43\110"));
	if enabled then
		if holdScript then
			holdScript.Disabled = true;
		end
		AddConnection(LUAOBFUSACTOR_DECRYPT_STR_0("\47\17\65\211\173\124\159\45\22\94\203\141\113\159\15\9\83\211\169\118\180", "\235\102\127\50\167\204\18") .. gui.Name, killButton.Activated:Connect(function()
			local parent = killButton.Parent.Parent;
			local target = parent.TargetPlayer.Value;
			local cause = parent.CauseOfDeath.Value;
			if (target and (cause ~= LUAOBFUSACTOR_DECRYPT_STR_0("\94\168\249", "\78\48\193\149\67\36"))) then
				LocalPlayer.PlayerGui.Click:Play();
				game.ReplicatedStorage.Remotes.Gamemode:FireServer({[LUAOBFUSACTOR_DECRYPT_STR_0("\3\10\129\12\84\35", "\33\80\126\224\120")]=LUAOBFUSACTOR_DECRYPT_STR_0("\201\176\6\199\73\248\173\37\214\83\225\140\6\197\72\228\134\12\208\89", "\60\140\200\99\164"),[LUAOBFUSACTOR_DECRYPT_STR_0("\179\245\22\33\167\147", "\194\231\148\100\70")]=target,[LUAOBFUSACTOR_DECRYPT_STR_0("\101\77\212\176\243", "\168\38\44\161\195\150")]=cause});
			end
		end));
		AddConnection(LUAOBFUSACTOR_DECRYPT_STR_0("\169\242\145\98\49\230\162\61\137\240\142\83\38\237\184\2\191", "\118\224\156\226\22\80\136\214") .. gui.Name, game.ReplicatedStorage.Remotes.Gamemode.OnClientEvent:Connect(function(arg1)
			if (arg1.Status == LUAOBFUSACTOR_DECRYPT_STR_0("\103\246\92\131\87\250\92\166\80\225\84\164\71\239\77\136\108\225\77\133", "\224\34\142\57")) then
				gui.Parent.Refresh:Fire();
				if arg1.Success then
					killButton.Text = LUAOBFUSACTOR_DECRYPT_STR_0("\237\146\230\254\86\194\110", "\110\190\199\165\189\19\145\61");
					killButton.Parent.Fill.Size = UDim2.new(1, 0, 1, 0);
					killButton.Parent.Fill.BackgroundColor3 = Color3.fromRGB(90, 202, 58);
					task.wait(1.5);
					killButton.Text = LUAOBFUSACTOR_DECRYPT_STR_0("\242\196\91\204\203\239\255\217\82\168\191\232\154\206\91\193\166\238\244\202\67\205", "\167\186\139\23\136\235");
					killButton.Parent.Fill.Size = UDim2.new(0, 0, 1, 0);
					killButton.Parent.Fill.BackgroundColor3 = Color3.fromRGB(90, 202, 58);
					return;
				end
				killButton.Text = LUAOBFUSACTOR_DECRYPT_STR_0("\46\135\177\77\59\146\169\36\52", "\109\122\213\232");
				killButton.Parent.Fill.Size = UDim2.new(1, 0, 1, 0);
				killButton.Parent.Fill.BackgroundColor3 = Color3.fromRGB(202, 60, 60);
				task.wait(1);
				killButton.Text = LUAOBFUSACTOR_DECRYPT_STR_0("\198\216\142\20\174\223\135\2\203\183\150\31\174\210\142\25\195\222\140\17\218\210", "\80\142\151\194");
				killButton.Parent.Fill.Size = UDim2.new(0, 0, 1, 0);
				killButton.Parent.Fill.BackgroundColor3 = Color3.fromRGB(90, 202, 58);
			end
		end));
	else
		if holdScript then
			holdScript.Disabled = false;
		end
		if Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\42\200\100\88\2\200\99\103\10\202\123\109\0\210\126\90\2\210\114\72\60", "\44\99\166\23") .. gui.Name] then
			Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\85\249\58\34\50\170\104\220\32\58\63\133\127\227\32\32\50\176\121\243\22", "\196\28\151\73\86\83") .. gui.Name]:Disconnect();
			Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\218\13\58\4\131\86\12\93\250\15\37\49\129\76\17\96\242\23\44\20\189", "\22\147\99\73\112\226\56\120") .. gui.Name] = nil;
		end
		if Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\145\123\241\225\140\182\97\201\252\129\180\80\244\240\131\172\74", "\237\216\21\130\149") .. gui.Name] then
			Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\171\64\76\75\177\199\74\169\71\83\83\149\223\91\140\90\96", "\62\226\46\63\63\208\169") .. gui.Name]:Disconnect();
			Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\204\23\70\151\30\3\59\117\236\21\89\166\9\8\33\74\218", "\62\133\121\53\227\127\109\79") .. gui.Name] = nil;
		end
	end
end
Options.InstantKill:OnChanged(function(enabled)
	local deathNoteGui = LocalPlayer.PlayerGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\52\17\51\225\222\128\173\4\17", "\194\112\116\82\149\182\206")) or LocalPlayer.PlayerGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\29\173\77\12\200\204\1\45\173\107\13\201", "\110\89\200\44\120\160\130"));
	if deathNoteGui then
		setupInstantKill(deathNoteGui, enabled);
	end
end);
AddConnection(LUAOBFUSACTOR_DECRYPT_STR_0("\140\214\66\98\70\94\62\78\191", "\45\203\163\43\38\35\42\91"), LocalPlayer.PlayerGui.ChildAdded:Connect(function(child)
	if (((child.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\246\128\221\55\143\135\91\198\128", "\52\178\229\188\67\231\201")) or (child.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\5\68\81\16\255\114\44\53\68\119\17\254", "\67\65\33\48\100\151\60"))) and Options.InstantKill.Value) then
		setupInstantKill(child, true);
	end
end));
local function setupDeathNoteStorage()
	if not workspace:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\236\243\171\217\255\203\239\133\209\225\222\212\186\215\225\222\224\171", "\147\191\135\206\184")) then
		storage = Instance.new(LUAOBFUSACTOR_DECRYPT_STR_0("\162\39\170\197\221\65", "\210\228\72\198\161\184\51"));
		storage.Name = LUAOBFUSACTOR_DECRYPT_STR_0("\5\93\246\17\127\218\62\98\250\2\114\253\34\70\225\17\116\203", "\174\86\41\147\112\19");
		storage.Parent = workspace;
	else
		storage = workspace.StealthKiraStorage;
	end
	local hasStoredGui = false;
	AddConnection(LUAOBFUSACTOR_DECRYPT_STR_0("\104\20\136\10\41\27\25\143\94\1\153\3\6\7\20\168\80", "\203\59\96\237\107\69\111\113"), LocalPlayer.CharacterRemoving:Connect(function()
		local storedGui = storage:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\0\19\173\245\57\222\216\48\19\139\244\56", "\183\68\118\204\129\81\144"));
		if storedGui then
			storedGui:Destroy();
		end
		hasStoredGui = false;
		if Options.StealthKira.Value then
			Options.StealthKira:SetValue(false);
		end
	end));
	AddConnection(LUAOBFUSACTOR_DECRYPT_STR_0("\61\185\117\229\7\150\6\138\101\237\47\135\26\168\115\240", "\226\110\205\16\132\107"), LocalPlayer.PlayerGui.ChildAdded:Connect(function(child)
		if ((child.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\207\198\225\205\73\197\204\244\220", "\33\139\163\128\185")) and not hasStoredGui) then
			task.wait(0.2);
			child.Name = LUAOBFUSACTOR_DECRYPT_STR_0("\115\93\5\202\95\118\11\202\82\127\17\215", "\190\55\56\100");
			child:SetAttribute(LUAOBFUSACTOR_DECRYPT_STR_0("\102\189\57\8\22\237\231\114\170\47\10\1\236\234", "\147\54\207\92\126\115\131"), true);
			child.Parent = storage;
			hasStoredGui = true;
			local button = LocalPlayer.PlayerGui.Main.StatsUI.Interact.DeathNote;
			if button then
				firesignal(button.MouseButton1Click);
			end
			Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\57\56\33\113\8", "\30\109\81\85\29\109")]="âœ… Death Note Stored",[LUAOBFUSACTOR_DECRYPT_STR_0("\220\126\90\162\51\208\232", "\156\159\17\52\214\86\190")]=LUAOBFUSACTOR_DECRYPT_STR_0("\157\251\184\189\162\251\181\252\133\230\175\189\238\253\184\189\170\246\252", "\220\206\143\221"),[LUAOBFUSACTOR_DECRYPT_STR_0("\162\104\63\22\204\197\221\136", "\178\230\29\77\119\184\172")]=3});
		end
	end));
end
setupDeathNoteStorage();
UserInputService.InputBegan:Connect(function(input, gameProcessed)
	if (not gameProcessed and (input.KeyCode == Enum.KeyCode[Options.StealthKiraKey.Value])) then
		Options.StealthKira:SetValue(not Options.StealthKira.Value);
	end
end);
Options.StealthKira:OnChanged(function(enabled)
	local storedGui = workspace.StealthKiraStorage:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\209\187\11\15\127\214\250\170\15\60\98\241", "\152\149\222\106\123\23"));
	local activeGui = LocalPlayer.PlayerGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\249\35\247\87\189\243\41\226\70\146\200\47", "\213\189\70\150\35"));
	if enabled then
		if activeGui then
			activeGui:Destroy();
		end
		if storedGui then
			storedGui.Parent = LocalPlayer.PlayerGui;
			local gamemode = ReplicatedStorage.Game.Gamemode.Value;
			if (storedGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\105\71\117\5\74", "\104\47\53\20")) and storedGui.Frame:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\133\94\128\17\185", "\111\195\44\225\124\220"))) then
				local writeFrame = storedGui.Frame.Frame:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\239\84\9\103\174", "\203\184\38\96\19\203"));
				local namesFrame = storedGui.Frame.Frame:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\23\114\116\68\221", "\174\89\19\25\33"));
				if (writeFrame and namesFrame) then
					if (gamemode == LUAOBFUSACTOR_DECRYPT_STR_0("\2\27\65\79", "\107\79\114\50\46\151\231")) then
						writeFrame.Visible = true;
						namesFrame.Visible = false;
					else
						writeFrame.Visible = false;
						namesFrame.Visible = true;
					end
				end
			end
		else
			Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\13\175\161\37\143", "\160\89\198\213\73\234\89\215")]="âŒ Error",[LUAOBFUSACTOR_DECRYPT_STR_0("\107\126\186\234\192\70\101", "\165\40\17\212\158")]=LUAOBFUSACTOR_DECRYPT_STR_0("\208\202\13\115\2\224\216\28\59\102\203\214\28\54\102\234\215\11\54\102\227\208\26\32\50\171", "\70\133\185\104\83"),[LUAOBFUSACTOR_DECRYPT_STR_0("\32\80\86\43\221\13\74\74", "\169\100\37\36\74")]=3});
			Options.StealthKira:SetValue(false);
		end
	elseif activeGui then
		activeGui.Parent = storage;
	end
end);
Tabs.KiraFeatures:AddSection(LUAOBFUSACTOR_DECRYPT_STR_0("\33\146\182\95\64\163\167\81\20\143\226\126\15\147\167\16\38\142\172\84\5\149", "\48\96\231\194"));
local function instantInteract(prompt)
	prompt.HoldDuration = 0;
	prompt.MaxActivationDistance = 20;
	prompt.RequiresLineOfSight = false;
	fireproximityprompt(prompt);
end
local FindDeathNote = Tabs.KiraFeatures:AddToggle(LUAOBFUSACTOR_DECRYPT_STR_0("\238\83\0\41\61\221\174\151\192\116\1\57\28", "\227\168\58\110\77\121\184\207"), {[LUAOBFUSACTOR_DECRYPT_STR_0("\79\53\171\76\180", "\197\27\92\223\32\209\187\17")]=LUAOBFUSACTOR_DECRYPT_STR_0("\34\74\215\244\67\121\202\245\7\31\231\254\2\75\203\187\45\80\215\254", "\155\99\63\163"),[LUAOBFUSACTOR_DECRYPT_STR_0("\166\212\167\140\172\136\150", "\228\226\177\193\237\217")]=false});
FindDeathNote:OnChanged(function(enabled)
	if not enabled then
		return;
	end
	spawn(function()
		local lastSearchedIndex = 0;
		while Options.FindDeathNote.Value do
			local Map = workspace:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\25\177\51", "\134\84\208\67"));
			if not Map then
				task.wait(2);
				continue;
			end
			if (#kiraUsernames > 0) then
				break;
			end
			local crates = {};
			for _, obj in pairs(Map:GetDescendants()) do
				if (obj.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\48\190\135\72\22", "\60\115\204\230")) then
					local prompt = obj:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\197\51\229\64\245\53\230\96\243", "\16\135\90\139"));
					if (prompt and prompt.Enabled) then
						table.insert(crates, obj);
					end
				end
			end
			if (#crates == 0) then
				task.wait(2);
				continue;
			end
			local cycleStart = tick();
			local processed = 0;
			local maxPerCycle = 5;
			while ((tick() - cycleStart) < 0.5) and Options.FindDeathNote.Value and (processed < maxPerCycle) do
				if (#kiraUsernames > 0) then
					break;
				end
				lastSearchedIndex = lastSearchedIndex + 1;
				if (lastSearchedIndex > #crates) then
					lastSearchedIndex = 1;
				end
				local currentCrate = crates[lastSearchedIndex];
				if (currentCrate and Players.LocalPlayer.Character and Players.LocalPlayer.Character:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\124\97\11\50\64\91\113\80\70\9\60\90\100\121\70\96", "\24\52\20\102\83\46\52"))) then
					local humanoidRootPart = Players.LocalPlayer.Character.HumanoidRootPart;
					local originalPosition = humanoidRootPart.CFrame;
					local originalCameraType = Camera.CameraType;
					local originalCameraCFrame = Camera.CFrame;
					Camera.CameraType = Enum.CameraType.Scriptable;
					local cratePosition = currentCrate.Position;
					local crateCFrame = currentCrate.CFrame;
					local lookVector = crateCFrame.LookVector;
					local characterOffset = 3;
					local cameraOffset = 2;
					local cameraHeight = 2;
					local characterPosition = cratePosition + (lookVector * characterOffset);
					humanoidRootPart.CFrame = CFrame.new(characterPosition, cratePosition);
					local cameraPosition = (characterPosition - (lookVector * cameraOffset)) + Vector3.new(0, cameraHeight, 0);
					Camera.CFrame = CFrame.new(cameraPosition, cratePosition);
					task.wait(0.1);
					local prompt = currentCrate:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\230\38\47\20\29\203\34\49\48", "\111\164\79\65\68"));
					if prompt then
						for i = 1, 3 do
							instantInteract(prompt);
							task.wait(0.1);
						end
					end
					task.wait(0.05);
					humanoidRootPart.CFrame = originalPosition;
					Camera.CameraType = originalCameraType;
					Camera.CFrame = originalCameraCFrame;
					processed = processed + 1;
				end
				task.wait(0.2);
			end
			if Options.FindDeathNote.Value then
				task.wait(2);
			end
		end
	end);
end);

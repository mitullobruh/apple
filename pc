local function updateKiraList(player)
	local username = player.Name;
	local kiraName = username .. LUAOBFUSACTOR_DECRYPT_STR_0("\29\62", "\45\61\22\19\124\19\203") .. player.DisplayName .. ")";
	local values = kiraList.Values or {};
	for _, value in ipairs(values) do
		if (value == kiraName) then
			return;
		end
	end
	table.insert(values, kiraName);
	kiraUsernames[username] = true;
	kiraList:SetValues(values);
	Fluent:Notify({[LUAOBFUSACTOR_DECRYPT_STR_0("\245\27\25\249\7", "\217\161\114\109\149\98\16")]="âš ï¸ KIRA DETECTED!",[LUAOBFUSACTOR_DECRYPT_STR_0("\49\47\54\104\185\122\6", "\20\114\64\88\28\220")]=(kiraName .. LUAOBFUSACTOR_DECRYPT_STR_0("\113\9\211\167\184\209\253\21\4\211\160\240\144\147\62\21\215\245", "\221\81\97\178\212\152\176")),[LUAOBFUSACTOR_DECRYPT_STR_0("\233\242\15\250\14\196\232\19", "\122\173\135\125\155")]=8});
end

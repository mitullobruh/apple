local function checkIntermission()
	local playerGui = LocalPlayer.PlayerGui;
	if (playerGui and playerGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\145\180\235\213\131\156", "\176\214\213\134"))) then
		local titleLabel = playerGui.GameUI.TopFrame.GPTitle.TextLabel;
		if (titleLabel and (titleLabel.Text == LUAOBFUSACTOR_DECRYPT_STR_0("\221\131\130\241\154\123\112\199\158\159\251\134", "\57\148\205\214\180\200\54"))) then
			clearKiraList();
		end
	end
end
local function setupIntermissionListener()
	local playerGui = LocalPlayer.PlayerGui;
	if (playerGui and playerGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\53\252\56\49\67\59", "\22\114\157\85\84"))) then
		local titleLabel = playerGui.GameUI.TopFrame.GPTitle.TextLabel;
		if titleLabel then
			titleLabel:GetPropertyChangedSignal(LUAOBFUSACTOR_DECRYPT_STR_0("\240\206\11\208", "\200\164\171\115\164\61\150")):Connect(function()
				if (titleLabel.Text == LUAOBFUSACTOR_DECRYPT_STR_0("\151\218\55\96\177\147\221\48\118\170\145\218", "\227\222\148\99\37")) then
					clearKiraList();
				end
			end);
		end
	end
end
setupIntermissionListener();

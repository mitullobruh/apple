local Noclipping;
local function UpdateNoClip()
	if not LocalPlayer.Character then
		return;
	end
	local alwaysNoCollide = {LUAOBFUSACTOR_DECRYPT_STR_0("\244\90\205\181", "\129\188\63\172\209\79\123\135"),LUAOBFUSACTOR_DECRYPT_STR_0("\108\225\224\217\102\235\233\217", "\173\32\132\134"),LUAOBFUSACTOR_DECRYPT_STR_0("\98\30\14\251\134\48\195\74", "\173\46\123\104\143\206\81"),LUAOBFUSACTOR_DECRYPT_STR_0("\152\24\36\158\105\140\22\177\15\3\152\72", "\97\212\125\66\234\37\227"),LUAOBFUSACTOR_DECRYPT_STR_0("\166\230\176\33\50\133\244\179\39\50\143\228", "\126\234\131\214\85"),LUAOBFUSACTOR_DECRYPT_STR_0("\168\208\79\78\122\148\197\76\72\110\150\216", "\47\228\181\41\58"),LUAOBFUSACTOR_DECRYPT_STR_0("\138\249\223\47\54\32\15\163\238\245\62\4", "\127\198\156\185\91\99\80"),LUAOBFUSACTOR_DECRYPT_STR_0("\199\19\203\248\179\45\54\209\225", "\190\149\122\172\144\199\107\89"),LUAOBFUSACTOR_DECRYPT_STR_0("\0\12\246\246\234\26\4\255\250", "\158\82\101\145\158"),LUAOBFUSACTOR_DECRYPT_STR_0("\66\247\5\30\80\92\241\21\19\86\81\236\15", "\36\16\158\98\118"),LUAOBFUSACTOR_DECRYPT_STR_0("\242\31\196\243\76\196\40\242\197\4\239\254\95", "\133\160\118\163\155\56\136\71"),LUAOBFUSACTOR_DECRYPT_STR_0("\196\171\118\250\162\42\165\230\167\99\211\164\18", "\213\150\194\17\146\214\127"),LUAOBFUSACTOR_DECRYPT_STR_0("\41\160\163\220\82\145\178\38\30\187\136\209\65", "\86\123\201\196\180\38\196\194")};
	local toggleParts = {LUAOBFUSACTOR_DECRYPT_STR_0("\194\248\201\170\229\220\214\189\228\231", "\207\151\136\185"),LUAOBFUSACTOR_DECRYPT_STR_0("\132\140\63\135\102\76\126\186\144\39", "\17\200\227\72\226\20\24"),LUAOBFUSACTOR_DECRYPT_STR_0("\152\84\22\214\199\254\230\251\130\78\20\195\249\240\253\235", "\159\208\33\123\183\169\145\143")};
	for _, partName in ipairs(alwaysNoCollide) do
		local part = LocalPlayer.Character:FindFirstChild(partName);
		if (part and part:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\208\91\43\51\194\91\42\34", "\86\146\58\88"))) then
			part.CanCollide = false;
		end
	end
	for _, partName in ipairs(toggleParts) do
		local part = LocalPlayer.Character:FindFirstChild(partName);
		if (part and part:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\122\222\249\197\158\232\36\238", "\154\56\191\138\160\206\137\86"))) then
			part.CanCollide = not Options.NoClip.Value;
		end
	end
end
UserInputService.InputBegan:Connect(function(input, gameProcessed)
	if (not gameProcessed and (input.KeyCode == Enum.KeyCode[Options.NoClipKey.Value])) then
		Options.NoClip:SetValue(not Options.NoClip.Value);
	end
end);
Options.NoClip:OnChanged(function(enabled)
	if enabled then
		if not Noclipping then
			Noclipping = RunService.Stepped:Connect(UpdateNoClip);
		end
	else
		if Noclipping then
			Noclipping:Disconnect();
			Noclipping = nil;
		end
		if LocalPlayer.Character then
			for _, partName in ipairs({LUAOBFUSACTOR_DECRYPT_STR_0("\179\73\229\130\110\14\142\222\149\86", "\172\230\57\149\231\28\90\225"),LUAOBFUSACTOR_DECRYPT_STR_0("\46\165\145\215\58\239\13\184\149\221", "\187\98\202\230\178\72"),LUAOBFUSACTOR_DECRYPT_STR_0("\9\244\169\49\68\46\232\160\2\69\46\245\148\49\88\53", "\42\65\129\196\80")}) do
				local part = LocalPlayer.Character:FindFirstChild(partName);
				if (part and part:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\32\75\78\223\39\6\16\250", "\142\98\42\61\186\119\103\98"))) then
					part.CanCollide = true;
				end
			end
		end
	end
end);
LocalPlayer.CharacterAdded:Connect(function()
	if Options.NoClip.Value then
		task.wait(0.1);
		UpdateNoClip();
	end
end);
local InvisibleMode = Tabs.Miscellaneous:AddToggle(LUAOBFUSACTOR_DECRYPT_STR_0("\17\177\20\1\43\182\0\4\61\146\13\12\61", "\104\88\223\98"), {[LUAOBFUSACTOR_DECRYPT_STR_0("\112\254\246\194\7", "\141\36\151\130\174\98")]=LUAOBFUSACTOR_DECRYPT_STR_0("\173\116\212\4\151\115\192\1\129\58\239\2\128\127", "\109\228\26\162"),[LUAOBFUSACTOR_DECRYPT_STR_0("\122\224\251\121\245\234\74", "\134\62\133\157\24\128")]=false});
local InvisibleKey = Tabs.Miscellaneous:AddKeybind(LUAOBFUSACTOR_DECRYPT_STR_0("\46\171\12\208\60\184\212\11\160\49\220\54", "\182\103\197\122\185\79\209"), {[LUAOBFUSACTOR_DECRYPT_STR_0("\199\142\245\123\5", "\40\147\231\129\23\96")]=LUAOBFUSACTOR_DECRYPT_STR_0("\92\246\154\76\168\165\222\121\253\204\110\190\181\222\124\246\136", "\188\21\152\236\37\219\204"),[LUAOBFUSACTOR_DECRYPT_STR_0("\109\230\51\9", "\108\32\137\87")]=LUAOBFUSACTOR_DECRYPT_STR_0("\158\231\7\161\35\252", "\57\202\136\96\198\79\153\43"),[LUAOBFUSACTOR_DECRYPT_STR_0("\143\38\172\166\152\171\236", "\152\203\67\202\199\237\199")]="C"});
local invisibleParts = {};
local character, humanoid, rootPart;
local invisibleEnabled = false;
local function updateInvisibleParts()
	character = LocalPlayer.Character;
	if character then
		humanoid = character:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\210\86\173\14\17\122\112\226", "\134\154\35\192\111\127\21\25"));
		rootPart = character:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\144\51\4\11\46\221\177\34\59\5\47\198\136\39\27\30", "\178\216\70\105\106\64"));
		invisibleParts = {};
		for _, part in pairs(character:GetDescendants()) do
			if (part:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\29\42\105\243\249\212\198\148", "\224\95\75\26\150\169\181\180")) and (part.Transparency == 0)) then
				table.insert(invisibleParts, part);
			end
		end
	end
end
updateInvisibleParts();
local function toggleInvisible(state)
	invisibleEnabled = state;
	for _, part in pairs(invisibleParts) do
		if (part and part.Parent) then
			part.Transparency = (invisibleEnabled and 0.5) or 0;
		end
	end
	if invisibleEnabled then
		AddConnection(LUAOBFUSACTOR_DECRYPT_STR_0("\34\212\206\33\87\165\116\7\223\244\39\75\188", "\22\107\186\184\72\36\204"), RunService.Heartbeat:Connect(function()
			if (not rootPart or not rootPart.Parent or not humanoid or not humanoid.Parent) then
				return;
			end
			local oldCFrame = rootPart.CFrame;
			local oldOffset = humanoid.CameraOffset;
			rootPart.CFrame = oldCFrame * CFrame.new(0, -200000, 0);
			humanoid.CameraOffset = rootPart.CFrame:ToObjectSpace(CFrame.new(oldCFrame.Position)).Position;
			RunService.RenderStepped:Wait();
			rootPart.CFrame = oldCFrame;
			humanoid.CameraOffset = oldOffset;
		end));
	elseif Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\206\179\50\71\29\238\191\40\75\34\232\178\52", "\110\135\221\68\46")] then
		Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\202\56\26\226\221\186\57\239\51\32\228\193\163", "\91\131\86\108\139\174\211")]:Disconnect();
		Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\210\37\174\30\78\242\41\180\18\113\244\36\168", "\61\155\75\216\119")] = nil;
	end
end
LocalPlayer.CharacterAdded:Connect(function(newChar)
	if Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\45\165\164\53\75\0\223\8\174\158\51\87\25", "\189\100\203\210\92\56\105")] then
		Connections[LUAOBFUSACTOR_DECRYPT_STR_0("\6\95\235\33\60\88\255\36\42\125\242\39\63", "\72\79\49\157")]:Disconnect();
	end
	character = newChar;
	humanoid = newChar:WaitForChild(LUAOBFUSACTOR_DECRYPT_STR_0("\160\165\60\189\134\191\56\184", "\220\232\208\81"));
	rootPart = newChar:WaitForChild(LUAOBFUSACTOR_DECRYPT_STR_0("\221\171\232\49\34\85\168\241\140\234\63\56\106\160\231\170", "\193\149\222\133\80\76\58"));
	invisibleParts = {};
	for _, part in pairs(newChar:GetDescendants()) do
		if (part:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\228\92\92\215\246\92\93\198", "\178\166\61\47")) and (part.Transparency == 0)) then
			table.insert(invisibleParts, part);
		end
	end
	if Options.InvisibleMode.Value then
		toggleInvisible(true);
	else
		invisibleEnabled = false;
	end
end);
Options.InvisibleMode:OnChanged(toggleInvisible);
UserInputService.InputBegan:Connect(function(input, gameProcessed)
	if (not gameProcessed and (input.KeyCode == Enum.KeyCode[Options.InvisibleKey.Value])) then
		Options.InvisibleMode:SetValue(not Options.InvisibleMode.Value);
	end
end);

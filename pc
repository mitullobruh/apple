local isInMatch = false;
local previousInMatch = false;
local function updateMatchState()
	local mapExists = workspace:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\238\125\247", "\54\163\28\135\114")) ~= nil;
	local inIntermission = false;
	local playerGui = LocalPlayer:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\24\215\92\155\75\109\15\206\84", "\31\72\187\61\226\46"));
	if playerGui then
		local gameUI = playerGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\228\7\78\215\114\87", "\68\163\102\35\178\39\30"));
		local topFrame = gameUI and gameUI:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\138\127\202\225\17\180\142\20", "\113\222\16\186\167\99\213\227"));
		local gpTitle = topFrame and topFrame:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\9\62\207\255\58\2\254", "\150\78\110\155"));
		local titleLabel = gpTitle and gpTitle:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\177\192\63\245\136\31\189\69\137", "\32\229\165\71\129\196\126\223"));
		if (titleLabel and (titleLabel.Text == LUAOBFUSACTOR_DECRYPT_STR_0("\234\167\240\164\179\248\234\186\247\168\174\251", "\181\163\233\164\225\225"))) then
			inIntermission = true;
		end
	end
	previousInMatch = isInMatch;
	isInMatch = mapExists and not inIntermission;
	if (previousInMatch and not isInMatch) then
		cleanupAllESP();
	end
end
updateMatchState();
workspace.ChildAdded:Connect(function(child)
	if (child.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\125\138\46", "\23\48\235\94")) then
		updateMatchState();
	end
end);
workspace.ChildRemoved:Connect(function(child)
	if (child.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\81\219\200", "\178\28\186\184\61\55\83")) then
		updateMatchState();
	end
end);
LocalPlayer.PlayerGui.ChildAdded:Connect(function(child)
	if (child.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\227\204\74\57\199\39", "\149\164\173\39\92\146\110")) then
		local topFrame = child:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\199\40\0\57\8\26\254\34", "\123\147\71\112\127\122"), true);
		local gpTitle = topFrame and topFrame:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\235\253\182\120\82\192\200", "\38\172\173\226\17"), true);
		local titleLabel = gpTitle and gpTitle:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\121\20\52\251\97\16\46\234\65", "\143\45\113\76"));
		if titleLabel then
			titleLabel:GetPropertyChangedSignal(LUAOBFUSACTOR_DECRYPT_STR_0("\140\189\4\40", "\92\216\216\124")):Connect(updateMatchState);
		end
		updateMatchState();
	end
end);
